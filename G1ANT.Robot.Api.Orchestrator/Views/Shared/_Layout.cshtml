@using G1ANT.Robot.Api.Orchestrator.Data
@model DefaultModel
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>G1ANT.Robot.Api.Orchestrator</title>
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.11.2/css/all.css">
    <link href="~/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/css/mdb-pro.css" rel="stylesheet">
    <link href="~/css/style.css" rel="stylesheet">
</head>

<body class="grey lighten-3">
    <header>
        <nav class="navbar fixed-top navbar-expand-lg navbar-light white scrolling-navbar">
            <div class="container-fluid">
                <a class="navbar-brand waves-effect" href="https://mdbootstrap.com/docs/jquery/" target="_blank">
                    <strong class="gray-text">G1ANT.Orchestrator</strong>
                </a>

                <!-- Collapse -->
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
                        aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <!-- Links -->
                <div class="collapse navbar-collapse" id="navbarSupportedContent">

                    <!-- Left -->
                    <ul class="navbar-nav mr-auto">
                        <li class="nav-item active">
                            <a class="nav-link waves-effect" href="#">
                                Home
                                <span class="sr-only">(current)</span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link waves-effect" href="https://g1ant.com/" target="_blank">
                                G1ANT.com
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link waves-effect" href="https://manual.g1ant.com" target="_blank">
                                Manual
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link waves-effect" href="https://github.com/G1ANT-Robot/G1ANT.Addon.Tutorials"
                               target="_blank">
                                Addon tutorials
                            </a>
                        </li>
                    </ul>

                    <ul class="navbar-nav nav-flex-icons">
                        <li class="nav-item">
                            <a href="https://www.facebook.com/G1ANTRPA/" class="nav-link waves-effect" target="_blank">
                                <i class="fab fa-facebook-f"></i>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="https://twitter.com/G1ANT_RPA" class="nav-link waves-effect" target="_blank">
                                <i class="fab fa-twitter"></i>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="https://www.youtube.com/channel/UCsm8dbf45slxwq_bkjkOkcA" class="nav-link waves-effect" target="_blank">
                                <i class="fab fa-youtube mr-3"></i>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="https://github.com/G1ANT-Robot" class="nav-link border border-light rounded waves-effect"
                               target="_blank">
                                <i class="fab fa-github mr-2"></i>G1ANT GitHub
                            </a>
                        </li>
                    </ul>

                </div>

            </div>
        </nav>

        <div class="sidebar-fixed position-fixed">

            <a class="logo-wrapper waves-effect">
                <img src="~/img/logo.png" class="img-fluid" alt="">
            </a>

                  <div class="list-group list-group-flush">
                      <!-- class="active" -->
                      <a href="~/" class="list-group-item list-group-item-action waves-effect">
                          <i class="fas fa-chart-pie mr-3"></i>Dashboard
                      </a>
                      <a href="~/Home/SubscribeAll" class="list-group-item list-group-item-action waves-effect">
                          <i class="fas fa-assistive-listening-systems mr-3"></i>Subscribe all
                      </a>
                      
                      <a href="#" class="list-group-item list-group-item-action waves-effect">
                          <i class="fas fa-angle-right mr-3"></i>Addon/List
                      </a>
                      <a href="#" class="list-group-item list-group-item-action waves-effect">
                          <i class="fas fa-angle-right mr-3"></i>Addon/Put
                      </a>
                      <a href="#" class="list-group-item list-group-item-action waves-effect">
                          <i class="fas fa-angle-right mr-3"></i>Processes/List
                      </a>
                      <a href="#" class="list-group-item list-group-item-action waves-effect">
                          <i class="fas fa-angle-right mr-3"></i>Processes/Get
                      </a>
                      <a href="#" class="list-group-item list-group-item-action waves-effect">
                          <i class="fas fa-angle-right mr-3"></i>Processes/Break
                      </a>
                      <a href="#" class="list-group-item list-group-item-action waves-effect">
                          <i class="fas fa-angle-right mr-3"></i>Processes/Run
                      </a>
                      <a href="#" class="list-group-item list-group-item-action waves-effect">
                          <i class="fas fa-angle-right mr-3"></i>Processes/Put
                      </a>
                      <a href="#" class="list-group-item list-group-item-action waves-effect">
                          <i class="fas fa-angle-right mr-3"></i>Triggers/Settings/Get
                      </a>
                      <a href="#" class="list-group-item list-group-item-action waves-effect">
                          <i class="fas fa-angle-right mr-3"></i>Triggers/Settings/Set
                      </a>
                      <a href="#" class="list-group-item list-group-item-action waves-effect">
                          <i class="fas fa-angle-right mr-3"></i>Triggers/Enable
                      </a>
                      <a href="#" class="list-group-item list-group-item-action waves-effect">
                          <i class="fas fa-angle-right mr-3"></i>Triggers/Disable
                      </a>
                      <a href="#" class="list-group-item list-group-item-action waves-effect">
                          <i class="fas fa-angle-right mr-3"></i>Subscriptions/Start
                      </a>
                      <a href="#" class="list-group-item list-group-item-action waves-effect">
                          <i class="fas fa-angle-right mr-3"></i>Subscriptions/Stop
                      </a>
                  </div>

        </div>
    </header>

    <main class="pt-5 mx-lg-5">
        <div class="container-fluid mt-5">
            @if (Model != null && !string.IsNullOrWhiteSpace(Model.Information))
            {
                <div class="card mb-4 wow fadeIn">
                    <div class="card-body">
                        <p class="card-text @Model.InformationClass">@Model.Information</p>
                    </div>
                </div>
            }
            <div class="card mb-4 wow fadeIn">
                <div class="card-body d-sm-flex justify-content-between">
                    <h4 class="mb-2 mb-sm-0 pt-1">
                        @RenderSection("topbar")
                    </h4>
                    <form class="d-flex justify-content-center" name="Search" action="~/" method="GET">
                        @if (!string.IsNullOrWhiteSpace(Data.SerialNumber))
                        {
                            <input type="search" placeholder="Enter serial number" aria-label="Search" class="form-control" name="SerialNumber" value="@Data.SerialNumber">
                        }
                        else
                        {
                            <input type="search" placeholder="Enter serial number" aria-label="Search" class="form-control" name="SerialNumber" >
                        }
                            <button class="btn btn-primary btn-sm my-0 p btn-dark-green" type="submit">
                                <i class="fas fa-search"></i>
                            </button>
                    </form>
                </div>
            </div>
            <div class="row wow fadeIn">
                @RenderBody()
            </div>
        </div>
</main>

    <footer class="page-footer text-center font-small white darken-3 mt-4 wow fadeIn">

        <div class="pt-4">
            <a class="btn btn-outline-grey" href="https://g1ant.com/" target="_blank"
               role="button">
                Download
                G1ANT.Studio
                <i class="fas fa-download ml-2"></i>
            </a>
        </div>

        <hr class="my-4">

        <div class="footer-copyright py-3">
            © 2019 Copyright:
            <a href="https://g1ant.com" target="_blank">G1ANT Robot Ltd</a>
        </div>

    </footer>
    <script type="text/javascript" src="~/js/jquery-3.4.1.min.js"></script>
    <script type="text/javascript" src="~/js/popper.min.js"></script>
    <script type="text/javascript" src="~/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/js/mdb.min.js"></script>
    <script type="text/javascript">
        new WOW().init();
    </script>
</body>
</html>
